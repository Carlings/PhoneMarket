@model List<Phone>

<div class="text-center">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Company</th>
                <th scope="col">Price</th>
                <th scope="col">Orders</th>
            </tr>
        </thead>
        @foreach(var item in Model)
        {
            <tbody>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Company</td>
                <td>@item.Price</td>
                <td>@item.Orders</td>
            </tbody>
        }
    </table>
</div>
